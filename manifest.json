{
    "name": "LinkedIn Connect",
    "manifest_version": 3,
    "description": "Accept all pending connections in one go",
    "version": "1.0",

   
    "action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/mynetwork/*"
            
            ],
            "js": ["content.js"]
        }
    ]
}
